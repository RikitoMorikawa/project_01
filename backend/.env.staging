# Staging Environment Configuration
ENVIRONMENT=staging

# Database Configuration (Aurora MySQL)
DB_HOST=${DB_HOST}
DB_PORT=3306
DB_NAME=csr_lambda_staging
DB_USER=${DB_USER}
DB_PASSWORD=${DB_PASSWORD}
DATABASE_URL=mysql+pymysql://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:3306/csr_lambda_staging

# AWS Configuration
AWS_REGION=ap-northeast-1
COGNITO_USER_POOL_ID=${COGNITO_USER_POOL_ID}
COGNITO_CLIENT_ID=${COGNITO_CLIENT_ID}

# API Configuration
API_TITLE=CSR Lambda API - Staging
CORS_ORIGINS=["https://staging.example.com"]